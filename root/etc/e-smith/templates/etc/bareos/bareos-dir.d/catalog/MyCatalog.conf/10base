{
use NethServer::Password;
my $password = NethServer::Password::store("bareos");
my $tcp = ${'rh-postgresql10-postgresql'}{TCPPort} || '55433';

$OUT.= qq(
Catalog \{
  Name = MyCatalog
  dbdriver = "postgresql"
  dbname = "bareos"
  dbuser = "bareos"
  dbpassword = "$password"
  dbport= "$tcp"
\}
)
}
